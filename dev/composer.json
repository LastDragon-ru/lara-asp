{
  "require": {
    "php": "^8.3|^8.4",
    "laravel/framework": "^12.0",
    "laravel/tinker": "^2.9",
    "lastdragon-ru/diy-parser": "self.version",
    "lastdragon-ru/glob-matcher": "self.version",
    "lastdragon-ru/lara-asp-dev": "self.version",
    "lastdragon-ru/lara-asp-documentator": "self.version",
    "lastdragon-ru/lara-asp-formatter": "self.version",
    "lastdragon-ru/lara-asp-graphql": "self.version",
    "lastdragon-ru/lara-asp-migrator": "self.version"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "LastDragon_ru\\DiyParser\\Docs\\": "vendor/lastdragon-ru/diy-parser/docs/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "lara-asp": {
      "type": "path",
      "url": "../packages/*"
    }
  }
}
