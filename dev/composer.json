{
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0",
    "laravel/tinker": "^2.9",
    "lastdragon-ru/lara-asp-dev": "self.version",
    "lastdragon-ru/lara-asp-documentator": "self.version",
    "lastdragon-ru/lara-asp-formatter": "self.version",
    "lastdragon-ru/lara-asp-graphql": "self.version",
    "lastdragon-ru/lara-asp-migrator": "self.version"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": false
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "core": {
      "type": "path",
      "url": "../packages/core"
    },
    "dev": {
      "type": "path",
      "url": "../packages/dev"
    },
    "documentator": {
      "type": "path",
      "url": "../packages/documentator"
    },
    "eloquent": {
      "type": "path",
      "url": "../packages/eloquent"
    },
    "formatter": {
      "type": "path",
      "url": "../packages/formatter"
    },
    "graphql": {
      "type": "path",
      "url": "../packages/graphql"
    },
    "graphql-printer": {
      "type": "path",
      "url": "../packages/graphql-printer"
    },
    "migrator": {
      "type": "path",
      "url": "../packages/migrator"
    },
    "serializer": {
      "type": "path",
      "url": "../packages/serializer"
    },
    "spa": {
      "type": "path",
      "url": "../packages/spa"
    },
    "testing": {
      "type": "path",
      "url": "../packages/testing"
    }
  }
}
