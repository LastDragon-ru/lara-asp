{
  "name": "lastdragon-ru/lara-asp-serializer",
  "homepage": "https://github.com/LastDragon-ru/lara-asp",
  "description": "The Awesome Set of Packages for Laravel - The Serializer.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "serializer",
    "laravel-package",
    "laravel",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/lara-asp/issues",
    "source": "https://github.com/LastDragon-ru/lara-asp",
    "forum": "https://github.com/LastDragon-ru/lara-asp/discussions"
  },
  "require": {
    "php": "^8.3|^8.4",
    "illuminate/contracts": "^12.0.1",
    "illuminate/database": "^12.0.1",
    "illuminate/support": "^12.0.1",
    "lastdragon-ru/lara-asp-core": "self.version",
    "phpdocumentor/type-resolver": "^1.7",
    "phpstan/phpdoc-parser": "^2.0.0",
    "symfony/polyfill-php84": "^1.31",
    "symfony/property-access": "^7.0.0",
    "symfony/property-info": "^7.1.10",
    "symfony/serializer": "^7.0.0"
  },
  "require-dev": {
    "lastdragon-ru/lara-asp-testing": "self.version",
    "mockery/mockery": "^1.6.6",
    "orchestra/testbench": "^10.0.0",
    "phpunit/phpunit": "^11.2.0|^12.0.0"
  },
  "autoload": {
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Serializer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Serializer\\Docs\\": "docs"
    },
    "exclude-from-classmap": [
      "docs/"
    ]
  },
  "conflict": {
    "thecodingmachine/safe": "<3.0.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LastDragon_ru\\LaraASP\\Serializer\\PackageProvider"
      ]
    },
    "lara-asp": {
      "ci": {
        "required-extensions": {
          "league/flysystem": [
            "ext-fileinfo"
          ]
        }
      }
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
