{
  "name": "lastdragon-ru/lara-asp-formatter",
  "homepage": "https://github.com/LastDragon-ru/lara-asp",
  "description": "The Awesome Set of Packages for Laravel - The Formatter.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "formatter",
    "laravel-package",
    "laravel",
    "intl",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/lara-asp/issues",
    "source": "https://github.com/LastDragon-ru/lara-asp",
    "forum": "https://github.com/LastDragon-ru/lara-asp/discussions"
  },
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "laravel/framework": "^8.22.1|^9.0.0",
    "lastdragon-ru/lara-asp-core": "self.version"
  },
  "require-dev": {
    "lastdragon-ru/lara-asp-testing": "self.version",
    "orchestra/testbench": "^6.9.0|^7.0.0",
    "phpunit/phpunit": "^9.5.0"
  },
  "suggest": {
    "ext-fileinfo": "Required for league/flysystem"
  },
  "autoload": {
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Formatter\\": "src/"
    },
    "exclude-from-classmap": [
      "src/**Test.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "LastDragon_ru\\LaraASP\\Formatter\\Provider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
