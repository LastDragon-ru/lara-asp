{
  "name": "lastdragon-ru/lara-asp-core",
  "version": "dev-master",
  "description": "The Awesome Set of Packages for Laravel - The Core.",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=7.4.0",
    "symfony/polyfill-php80": "^1.20"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Core\\": "src/"
    },
    "optimize-autoloader": true,
    "exclude-from-classmap": [
      "src/**Test.php"
    ]
  }
}
