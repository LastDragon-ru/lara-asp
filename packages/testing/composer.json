{
  "name": "lastdragon-ru/lara-asp-testing",
  "homepage": "https://github.com/LastDragon-ru/lara-asp",
  "description": "The Awesome Set of Packages for Laravel - Testing Helpers.",
  "readme": "readme.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "testing",
    "laravel-package",
    "laravel",
    "phpunit",
    "phpunit-assertions",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/lara-asp/issues",
    "source": "https://github.com/LastDragon-ru/lara-asp"
  },
  "require": {
    "php": ">=7.4.0",
    "ext-json": "*",
    "ext-xml": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-xmlreader": "*",
    "doctrine/dbal": "^2.12|^3.0",
    "http-interop/http-factory-guzzle": "^1.0",
    "laravel/framework": "^8.0",
    "m1x0n/opis-json-schema-error-presenter": "^0.5.2",
    "opis/json-schema": "^1.0",
    "psr/http-message": "^1.0",
    "symfony/psr-http-message-bridge": "^2.0"
  },
  "require-dev": {
    "guzzlehttp/psr7": "^1.7",
    "phpunit/phpunit": "^9.0"
  },
  "conflict": {
    "phpunit/phpunit": "<9.0"
  },
  "suggest": {
    "orchestra/testbench": "For testing the packages which required Application."
  },
  "autoload": {
    "files": [
      "src/mixins.php"
    ],
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Testing\\": "src/"
    },
    "exclude-from-classmap": [
      "src/**Test.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
