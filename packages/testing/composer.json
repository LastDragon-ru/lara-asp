{
  "name": "lastdragon-ru/lara-asp-testing",
  "homepage": "https://github.com/LastDragon-ru/lara-asp",
  "description": "The Awesome Set of Packages for Laravel - Testing Helpers.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "testing",
    "laravel-package",
    "laravel",
    "phpunit",
    "phpunit-assertions",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/lara-asp/issues",
    "source": "https://github.com/LastDragon-ru/lara-asp",
    "forum": "https://github.com/LastDragon-ru/lara-asp/discussions"
  },
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-xmlreader": "*",
    "ext-mbstring": "*",
    "doctrine/dbal": "^3.1.4",
    "doctrine/sql-formatter": "^1.1",
    "http-interop/http-factory-guzzle": "^1.0.0",
    "laravel/framework": "^10.0.0",
    "mockery/mockery": "^1.6.2",
    "opis/json-schema": "^2.3.0",
    "phpunit/phpunit": "^10.1.0",
    "psr/http-message": "^1.0.0|^2.0.0",
    "sebastian/comparator": "^5.0",
    "sebastian/exporter": "^5.0",
    "symfony/filesystem": "^6.3.0",
    "symfony/http-foundation": "^6.3.0",
    "symfony/mime": "^6.3.0",
    "symfony/psr-http-message-bridge": "^2.0.0|^6.4.0",
    "symfony/polyfill-php83": "^1.28"
  },
  "require-dev": {
    "guzzlehttp/psr7": "^1.9.1|^2.4.5",
    "orchestra/testbench": "^8.0.0"
  },
  "suggest": {
    "ext-fileinfo": "Required for league/flysystem",
    "orchestra/testbench": "For testing the packages which required Application."
  },
  "autoload": {
    "files": [
      "src/mixins.php"
    ],
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Testing\\": "src/"
    },
    "exclude-from-classmap": [
      "src/**Test.php",
      "src/Package/**.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
