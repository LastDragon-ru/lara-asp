{
  "name": "lastdragon-ru/lara-asp-testing",
  "homepage": "https://github.com/LastDragon-ru/lara-asp",
  "description": "The Awesome Set of Packages for Laravel - Testing Helpers.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "testing",
    "laravel-package",
    "laravel",
    "phpunit",
    "phpunit-assertions",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/lara-asp/issues",
    "source": "https://github.com/LastDragon-ru/lara-asp",
    "forum": "https://github.com/LastDragon-ru/lara-asp/discussions"
  },
  "require": {
    "php": "^8.2|^8.3",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-xmlreader": "*",
    "ext-mbstring": "*",
    "composer/semver": "^3.2",
    "doctrine/sql-formatter": "^1.1",
    "http-interop/http-factory-guzzle": "^1.0.0",
    "illuminate/collections": "^10.34.0|^11.0.0",
    "illuminate/console": "^10.34.0|^11.0.0",
    "illuminate/contracts": "^10.34.0|^11.0.0",
    "illuminate/database": "^10.34.0|^11.0.0",
    "illuminate/testing": "^10.34.0|^11.0.0",
    "illuminate/translation": "^10.34.0|^11.0.0",
    "mockery/mockery": "^1.6.2",
    "opis/json-schema": "^2.3.0",
    "phpunit/phpunit": "^10.1.0|^11.0.0",
    "psr/http-message": "^1.0.0|^2.0.0",
    "sebastian/comparator": "^5.0|^6.0.0",
    "sebastian/exporter": "^5.0|^6.0.0",
    "symfony/deprecation-contracts": "^3.0.0",
    "symfony/filesystem": "^6.3.0|^7.0.0",
    "symfony/http-foundation": "^6.3.0|^7.0.0",
    "symfony/mime": "^6.3.0|^7.0.0",
    "symfony/psr-http-message-bridge": "^2.0.0|^6.4.0|^7.0.0",
    "symfony/polyfill-php83": "^1.28"
  },
  "require-dev": {
    "fakerphp/faker": "^1.21.0",
    "guzzlehttp/psr7": "^2.4.5",
    "illuminate/http": "^10.34.0|^11.0.0",
    "laravel/scout": "^9.8.0|^10.0.0",
    "orchestra/testbench": "^8.0.0|^9.0.0",
    "symfony/console": "^6.3.0|^7.0.0",
    "symfony/http-kernel": "^6.3.0|^7.0.0"
  },
  "autoload": {
    "files": [
      "src/mixins.php"
    ],
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Testing\\": "src/"
    },
    "exclude-from-classmap": [
      "src/**Test.php",
      "src/Package/**.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Testing\\Docs\\": "docs"
    },
    "exclude-from-classmap": [
      "docs/"
    ]
  },
  "extra": {
    "lara-asp": {
      "ci": {
        "required-extensions": {
          "league/flysystem": [
            "ext-fileinfo"
          ],
          "illuminate/console": [
            "ext-mbstring"
          ]
        }
      }
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
