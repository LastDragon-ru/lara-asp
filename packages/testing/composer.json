{
  "name": "lastdragon-ru/lara-asp-testing",
  "homepage": "https://github.com/LastDragon-ru/lara-asp",
  "description": "The Awesome Set of Packages for Laravel - Testing Helpers.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "testing",
    "laravel-package",
    "laravel",
    "phpunit",
    "phpunit-assertions",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/lara-asp/issues",
    "source": "https://github.com/LastDragon-ru/lara-asp",
    "forum": "https://github.com/LastDragon-ru/lara-asp/discussions"
  },
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "ext-json": "*",
    "ext-xml": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-xmlreader": "*",
    "ext-mbstring": "*",
    "doctrine/dbal": "^2.12.0|^3.1.4",
    "doctrine/sql-formatter": "^1.1",
    "http-interop/http-factory-guzzle": "^1.0.0",
    "laravel/framework": "^9.0.0|^10.0.0",
    "mockery/mockery": "^1.4.2",
    "opis/json-schema": "^2.1.0",
    "psr/http-message": "^1.0.0",
    "symfony/filesystem": "^5.2.0|^6.0.0",
    "symfony/http-foundation": "^5.2.0|^6.0.0",
    "symfony/mime": "^5.3.0|^6.0.0",
    "symfony/psr-http-message-bridge": "^2.0.0"
  },
  "require-dev": {
    "guzzlehttp/psr7": "^1.8.5|^2.1.1",
    "phpunit/phpunit": "^9.5.0|^10.0.15",
    "orchestra/testbench": "^6.9.0|^7.0.0|^8.0.0"
  },
  "conflict": {
    "phpunit/phpunit": "<9.0"
  },
  "suggest": {
    "ext-fileinfo": "Required for league/flysystem",
    "orchestra/testbench": "For testing the packages which required Application."
  },
  "autoload": {
    "files": [
      "src/mixins.php"
    ],
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Testing\\": "src/"
    },
    "exclude-from-classmap": [
      "src/**Test.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
