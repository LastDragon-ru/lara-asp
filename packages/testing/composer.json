{
  "name": "lastdragon-ru/lara-asp-testing",
  "version": "dev-master",
  "description": "The Awesome Set of Packages for Laravel - Testing Helpers.",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=7.4.0",
    "laravel/framework": "^8.0",
    "doctrine/dbal": "^2.12"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Testing\\": "src/"
    },
    "optimize-autoloader": true,
    "exclude-from-classmap": [
      "src/**Test.php"
    ]
  }
}
