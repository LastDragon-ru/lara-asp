{
  "name": "lastdragon-ru/lara-asp-testing",
  "homepage": "https://github.com/LastDragon-ru/lara-asp",
  "description": "The Awesome Set of Packages for Laravel - Testing Helpers.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "testing",
    "laravel-package",
    "laravel",
    "phpunit",
    "phpunit-assertions",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/lara-asp/issues",
    "source": "https://github.com/LastDragon-ru/lara-asp",
    "forum": "https://github.com/LastDragon-ru/lara-asp/discussions"
  },
  "require": {
    "php": "^8.3|^8.4",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-xmlreader": "*",
    "ext-mbstring": "*",
    "composer/semver": "^3.2",
    "doctrine/sql-formatter": "^1.1",
    "illuminate/collections": "^12.0.1",
    "illuminate/console": "^12.0.1",
    "illuminate/contracts": "^12.0.1",
    "illuminate/database": "^12.0.1",
    "illuminate/testing": "^12.0.1",
    "illuminate/translation": "^12.0.1",
    "nyholm/psr7": "^1.8",
    "mockery/mockery": "^1.6.6",
    "opis/json-schema": "^2.3.0",
    "phpunit/phpunit": "^11.1.0|^12.0.0",
    "psr/http-message": "^1.0.0|^2.0.0",
    "sebastian/comparator": "^6.0.0|^7.0.0",
    "sebastian/exporter": "^6.0.0|^7.0.0",
    "symfony/filesystem": "^7.0.0",
    "symfony/finder": "^7.0.0",
    "symfony/http-foundation": "^7.0.0",
    "symfony/mime": "^7.0.0",
    "symfony/polyfill-php84": "^1.31",
    "symfony/psr-http-message-bridge": "^7.0.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.21.0",
    "illuminate/http": "^12.0.1",
    "laravel/scout": "^10.12.2",
    "orchestra/testbench": "^10.0.0",
    "symfony/console": "^7.0.0"
  },
  "autoload": {
    "files": [
      "src/mixins.php"
    ],
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Testing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Testing\\Docs\\": "docs"
    },
    "exclude-from-classmap": [
      "docs/"
    ]
  },
  "extra": {
    "lara-asp": {
      "ci": {
        "required-extensions": {
          "league/flysystem": [
            "ext-fileinfo"
          ],
          "illuminate/console": [
            "ext-mbstring"
          ]
        }
      }
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
