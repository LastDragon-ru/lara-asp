{
  "name": "lastdragon-ru/graphql-printer",
  "homepage": "https://github.com/LastDragon-ru/lara-asp",
  "description": "The Awesome Set of Packages for Laravel - The GraphQL Printer.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "GraphQL",
    "GraphQL Printer",
    "GraphQL Schema Printer",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/lara-asp/issues",
    "source": "https://github.com/LastDragon-ru/lara-asp",
    "forum": "https://github.com/LastDragon-ru/lara-asp/discussions"
  },
  "suggest": {
    "lastdragon-ru/graphql-printer-testing": "Useful assertions for PHPUnit to check printed/exported type/queries."
  },
  "require": {
    "php": "^8.3|^8.4",
    "ext-mbstring": "*",
    "symfony/deprecation-contracts": "^3.0.0",
    "symfony/polyfill-php84": "^1.31",
    "composer/semver": "^3.2",
    "webonyx/graphql-php": "^15.4.0"
  },
  "require-dev": {
    "composer/class-map-generator": "^1.0",
    "phpunit/phpunit": "^11.2.0|^12.0.0",
    "lastdragon-ru/graphql-printer-testing": "self.version",
    "lastdragon-ru/lara-asp-testing": "self.version",
    "mockery/mockery": "^1.6.6"
  },
  "autoload": {
    "psr-4": {
      "LastDragon_ru\\GraphQLPrinter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LastDragon_ru\\GraphQLPrinter\\Docs\\": "docs"
    },
    "exclude-from-classmap": [
      "docs/"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
