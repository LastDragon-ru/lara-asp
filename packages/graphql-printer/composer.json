{
  "name": "lastdragon-ru/graphql-printer",
  "homepage": "https://github.com/LastDragon-ru/php-packages",
  "description": "The package allows you to print GraphQL Schema and Queries in a highly customized way eg you can choose indent size, print only used/wanted/all types, print only one type, print used/wanted/all directives and even check which types/directives are used in the Schema/Query.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "GraphQL",
    "GraphQL Printer",
    "GraphQL Schema Printer",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/php-packages/issues",
    "source": "https://github.com/LastDragon-ru/php-packages",
    "forum": "https://github.com/LastDragon-ru/php-packages/discussions"
  },
  "suggest": {
    "lastdragon-ru/phpunit-graphql": "Useful assertions for PHPUnit to check printed/exported type/queries."
  },
  "require": {
    "php": "^8.3|^8.4",
    "ext-mbstring": "*",
    "symfony/deprecation-contracts": "^3.0.0",
    "symfony/polyfill-php84": "^1.31",
    "composer/semver": "^3.2",
    "webonyx/graphql-php": "^15.4.0"
  },
  "require-dev": {
    "composer/class-map-generator": "^1.0",
    "phpunit/phpunit": "^11.3.0|^12.0.0",
    "lastdragon-ru/phpunit-extensions": "self.version",
    "lastdragon-ru/phpunit-graphql": "self.version",
    "mockery/mockery": "^1.6.6"
  },
  "autoload": {
    "psr-4": {
      "LastDragon_ru\\GraphQLPrinter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LastDragon_ru\\GraphQLPrinter\\Docs\\": "docs"
    },
    "exclude-from-classmap": [
      "docs/"
    ]
  },
  "conflict": {
    "thecodingmachine/safe": "<3.0.0"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
