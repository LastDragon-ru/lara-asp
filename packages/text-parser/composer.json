{
  "name": "lastdragon-ru/text-parser",
  "homepage": "https://github.com/LastDragon-ru/php-packages",
  "description": "Yet another package to simplify writing fast and memory-effective parsers that can parse infinite strings.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "parser",
    "php",
    "diy"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/php-packages/issues",
    "source": "https://github.com/LastDragon-ru/php-packages",
    "forum": "https://github.com/LastDragon-ru/php-packages/discussions"
  },
  "require": {
    "php": "^8.3|^8.4",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "lastdragon-ru/phpunit-extensions": "self.version",
    "mockery/mockery": "^1.6.6",
    "phpunit/phpunit": "^11.3.0|^12.0.0"
  },
  "autoload": {
    "psr-4": {
      "LastDragon_ru\\TextParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LastDragon_ru\\TextParser\\Docs\\": "docs/"
    },
    "exclude-from-classmap": [
      "docs/"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
