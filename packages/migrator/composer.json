{
  "name": "lastdragon-ru/lara-asp-migrator",
  "homepage": "https://github.com/LastDragon-ru/lara-asp",
  "description": "The Awesome Set of Packages for Laravel - The Migrator.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "database",
    "database migration",
    "laravel-raw-sql-migrator",
    "laravel-migrator",
    "laravel-package",
    "laravel",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/lara-asp/issues",
    "source": "https://github.com/LastDragon-ru/lara-asp",
    "forum": "https://github.com/LastDragon-ru/lara-asp/discussions"
  },
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "ext-intl": "*",
    "ext-json": "*",
    "composer/semver": "^3.2",
    "laravel/framework": "^9.0.0|^10.0.0",
    "lastdragon-ru/lara-asp-core": "self.version",
    "symfony/filesystem": "^5.2.0|^6.0.0",
    "symfony/finder": "^5.1.0|^6.0.0",
    "doctrine/dbal": "^2.12.0|^3.1.4"
  },
  "require-dev": {
    "ext-pdo_sqlite": "*",
    "lastdragon-ru/lara-asp-testing": "self.version",
    "mockery/mockery": "^1.4.4",
    "orchestra/testbench": "^6.9.0|^7.0.0|^8.0.0",
    "phpunit/phpunit": "^9.5.0|^10.1.0"
  },
  "suggest": {
    "ext-fileinfo": "Required for league/flysystem"
  },
  "autoload": {
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Migrator\\": "src/"
    },
    "exclude-from-classmap": [
      "src/**Test.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "LastDragon_ru\\LaraASP\\Migrator\\Provider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
