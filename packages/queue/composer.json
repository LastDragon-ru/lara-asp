{
  "name": "lastdragon-ru/lara-asp-queue",
  "homepage": "https://github.com/LastDragon-ru/lara-asp",
  "description": "The Awesome Set of Packages for Laravel - Queue Helpers.",
  "readme": "readme.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "queue",
    "laravel-queue",
    "laravel-package",
    "laravel",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/lara-asp/issues",
    "source": "https://github.com/LastDragon-ru/lara-asp"
  },
  "require": {
    "php": ">=7.4.0",
    "laravel/framework": "^8.2.0",
    "lastdragon-ru/lara-asp-core": "self.version"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0"
  },
  "suggest": {
    "hmazter/laravel-schedule-list": "Package to add a artisan command to list all scheduled artisan commands."
  },
  "autoload": {
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Queue\\": "src/"
    },
    "exclude-from-classmap": [
      "src/**Test.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
