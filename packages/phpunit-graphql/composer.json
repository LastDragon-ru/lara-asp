{
  "name": "lastdragon-ru/phpunit-graphql",
  "homepage": "https://github.com/LastDragon-ru/php-packages",
  "description": "PHPUnit assertions for GraphQL to check printed/exported type/queries.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "GraphQL",
    "GraphQL Printer",
    "GraphQL Schema Printer",
    "PHPUnit",
    "php"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/php-packages/issues",
    "source": "https://github.com/LastDragon-ru/php-packages",
    "forum": "https://github.com/LastDragon-ru/php-packages/discussions"
  },
  "require": {
    "php": "^8.3|^8.4",
    "lastdragon-ru/graphql-printer": "self.version",
    "phpunit/phpunit": "^11.2.0|^12.0.0",
    "webonyx/graphql-php": "^15.4.0"
  },
  "require-dev": {
    "lastdragon-ru/phpunit-extensions": "self.version",
    "mockery/mockery": "^1.6.6"
  },
  "autoload": {
    "psr-4": {
      "LastDragon_ru\\PhpUnit\\GraphQL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LastDragon_ru\\PhpUnit\\GraphQL\\Docs\\": "docs/"
    },
    "exclude-from-classmap": [
      "docs/"
    ]
  },
  "conflict": {
    "thecodingmachine/safe": "<3.0.0"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
