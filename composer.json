{
  "name": "lastdragon-ru/lara-asp",
  "homepage": "https://github.com/LastDragon-ru/lara-asp",
  "description": "The Awesome Set of Packages for Laravel.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "intl",
    "formatter",
    "testing",
    "queue",
    "laravel-queue",
    "database",
    "database migration",
    "laravel-raw-sql-migrator",
    "laravel-migrator",
    "laravel-eloquent",
    "laravel-package",
    "laravel",
    "phpunit",
    "phpunit-assertions",
    "php",
    "spa",
    "angular",
    "GraphQL",
    "lighthouse-php",
    "serializer"
  ],
  "support": {
    "issues": "https://github.com/LastDragon-ru/lara-asp/issues",
    "source": "https://github.com/LastDragon-ru/lara-asp",
    "forum": "https://github.com/LastDragon-ru/lara-asp/discussions"
  },
  "require": {
    "php": "^8.3|^8.4",
    "ext-bcmath": "*",
    "ext-dom": "*",
    "ext-filter": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-xmlreader": "*",
    "composer/semver": "^3.2",
    "doctrine/sql-formatter": "^1.1",
    "fakerphp/faker": "^1.21.0",
    "guzzlehttp/psr7": "^2.4.5",
    "illuminate/collections": "^12.0.1",
    "illuminate/conditionable": "^12.0.1",
    "illuminate/console": "^12.0.1",
    "illuminate/container": "^12.0.1",
    "illuminate/contracts": "^12.0.1",
    "illuminate/database": "^12.0.1",
    "illuminate/filesystem": "^12.0.1",
    "illuminate/http": "^12.0.1",
    "illuminate/macroable": "^12.0.1",
    "illuminate/pagination": "^12.0.1",
    "illuminate/process": "^12.0.1",
    "illuminate/routing": "^12.0.1",
    "illuminate/support": "^12.0.1",
    "illuminate/testing": "^12.0.1",
    "illuminate/translation": "^12.0.1",
    "illuminate/validation": "^12.0.1",
    "league/commonmark": "^2.6.0",
    "league/config": "^1.1.1",
    "mockery/mockery": "^1.6.6",
    "nette/neon": "^3.4.1",
    "nikic/php-parser": "^5.4.0",
    "nuwave/lighthouse": "^6.54.0",
    "nyholm/psr7": "^1.8",
    "opis/json-schema": "^2.3.0",
    "phpdocumentor/type-resolver": "^1.7",
    "phpstan/phpdoc-parser": "^2.0.0",
    "phpstan/phpstan": "^2.1.1",
    "phpunit/phpunit": "^11.2.0|^12.0.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-message": "^1.0.0|^2.0.0",
    "sebastian/comparator": "^6.0.0|^7.0.0",
    "sebastian/exporter": "^6.0.0|^7.0.0",
    "symfony/console": "^7.0.0",
    "symfony/deprecation-contracts": "^3.0.0",
    "symfony/filesystem": "^7.1.0",
    "symfony/finder": "^7.0.0",
    "symfony/http-foundation": "^7.0.0",
    "symfony/http-kernel": "^7.0.0",
    "symfony/mime": "^7.0.0",
    "symfony/polyfill-php84": "^1.31",
    "symfony/polyfill-php85": "^1.33",
    "symfony/property-access": "^7.0.0",
    "symfony/property-info": "^7.1.10",
    "symfony/psr-http-message-bridge": "^7.0.0",
    "symfony/serializer": "^7.0.0",
    "symfony/var-dumper": "^7.0.0",
    "webonyx/graphql-php": "^15.4.0"
  },
  "require-dev": {
    "ext-xmlwriter": "*",
    "ext-pdo_sqlite": "*",
    "bamarni/composer-bin-plugin": "^1.8",
    "composer/class-map-generator": "^1.0",
    "illuminate/config": "^12.0.1",
    "laravel/scout": "^10.12.2",
    "larastan/larastan": "^3.1.0",
    "orchestra/testbench": "^10.0.0",
    "phpstan/phpstan": "2.1.33",
    "phpstan/phpstan-mockery": "^2.0.0",
    "phpstan/phpstan-phpunit": "2.0.9",
    "phpstan/phpstan-strict-rules": "^2.0.1",
    "spaze/phpstan-disallowed-calls": "^4.0.1"
  },
  "autoload": {
    "files": [
      "packages/lara-asp-eloquent/src/mixins.php",
      "packages/lara-asp-testing/src/mixins.php"
    ],
    "psr-4": {
      "LastDragon_ru\\GlobMatcher\\": "packages/glob-matcher/src/",
      "LastDragon_ru\\GraphQLPrinter\\": "packages/graphql-printer/src/",
      "LastDragon_ru\\LaraASP\\Core\\": "packages/lara-asp-core/src/",
      "LastDragon_ru\\LaraASP\\Dev\\": "packages/lara-asp-dev/src/",
      "LastDragon_ru\\LaraASP\\Documentator\\": "packages/lara-asp-documentator/src/",
      "LastDragon_ru\\LaraASP\\Eloquent\\": "packages/lara-asp-eloquent/src/",
      "LastDragon_ru\\LaraASP\\Formatter\\": "packages/lara-asp-formatter/src/",
      "LastDragon_ru\\LaraASP\\GraphQL\\": "packages/lara-asp-graphql/src/",
      "LastDragon_ru\\LaraASP\\GraphQL\\Testing\\": "packages/lara-asp-graphql-testing/src/",
      "LastDragon_ru\\LaraASP\\Migrator\\": "packages/lara-asp-migrator/src/",
      "LastDragon_ru\\LaraASP\\Serializer\\": "packages/lara-asp-serializer/src/",
      "LastDragon_ru\\LaraASP\\Spa\\": "packages/lara-asp-spa/src/",
      "LastDragon_ru\\LaraASP\\Testing\\": "packages/lara-asp-testing/src/",
      "LastDragon_ru\\Path\\": "packages/path/src/",
      "LastDragon_ru\\PhpUnit\\": "packages/phpunit-extensions/src/",
      "LastDragon_ru\\PhpUnit\\GraphQL\\": "packages/phpunit-graphql/src/",
      "LastDragon_ru\\TextParser\\": "packages/text-parser/src/"
    }
  },
  "autoload-dev": {
    "exclude-from-classmap": [
      "packages/glob-matcher/docs/",
      "packages/graphql-printer/docs/",
      "packages/lara-asp-core/docs/",
      "packages/lara-asp-dev/docs/",
      "packages/lara-asp-documentator/docs/",
      "packages/lara-asp-eloquent/docs/",
      "packages/lara-asp-formatter/docs/",
      "packages/lara-asp-graphql/docs/",
      "packages/lara-asp-migrator/docs/",
      "packages/lara-asp-migrator/src/Migrations/MigratorTest/raw/",
      "packages/lara-asp-serializer/docs/",
      "packages/lara-asp-spa/docs/",
      "packages/lara-asp-testing/docs/",
      "packages/path/docs/",
      "packages/phpunit-extensions/docs/",
      "packages/phpunit-graphql/docs/",
      "packages/text-parser/docs/",
      "packages/**/*Test/"
    ],
    "psr-4": {
      "LastDragon_ru\\GlobMatcher\\Docs\\": "packages/glob-matcher/docs",
      "LastDragon_ru\\GraphQLPrinter\\Docs\\": "packages/graphql-printer/docs",
      "LastDragon_ru\\LaraASP\\Core\\Docs\\": "packages/lara-asp-core/docs",
      "LastDragon_ru\\LaraASP\\Dev\\Docs\\": "packages/lara-asp-dev/docs",
      "LastDragon_ru\\LaraASP\\Documentator\\Docs\\": "packages/lara-asp-documentator/docs",
      "LastDragon_ru\\LaraASP\\Eloquent\\Docs\\": "packages/lara-asp-eloquent/docs",
      "LastDragon_ru\\LaraASP\\Formatter\\Docs\\": "packages/lara-asp-formatter/docs",
      "LastDragon_ru\\LaraASP\\GraphQL\\Docs\\": "packages/lara-asp-graphql/docs",
      "LastDragon_ru\\LaraASP\\GraphQL\\Testing\\Docs\\": "packages/lara-asp-graphql-testing/docs",
      "LastDragon_ru\\LaraASP\\Migrator\\Docs\\": "packages/lara-asp-migrator/docs",
      "LastDragon_ru\\LaraASP\\Serializer\\Docs\\": "packages/lara-asp-serializer/docs",
      "LastDragon_ru\\LaraASP\\Spa\\Docs\\": "packages/lara-asp-spa/docs",
      "LastDragon_ru\\LaraASP\\Testing\\Docs\\": "packages/lara-asp-testing/docs",
      "LastDragon_ru\\Path\\Docs\\": "packages/path/docs",
      "LastDragon_ru\\PhpUnit\\Docs\\": "packages/phpunit-extensions/docs",
      "LastDragon_ru\\PhpUnit\\GraphQL\\Docs\\": "packages/phpunit-graphql/docs",
      "LastDragon_ru\\TextParser\\Docs\\": "packages/text-parser/docs"
    }
  },
  "conflict": {
    "thecodingmachine/safe": "<3.0.0"
  },
  "extra": {
    "bamarni-bin": {
      "bin-links": false,
      "forward-command": false
    },
    "lara-asp": {
      "ci": {
        "optional-packages": [
          "laravel/scout"
        ]
      },
      "sync-packages": {
        "lara-asp-core": [
          "core"
        ],
        "lara-asp-dev": [
          "dev"
        ],
        "lara-asp-documentator": [
          "documentator"
        ],
        "lara-asp-eloquent": [
          "eloquent"
        ],
        "lara-asp-formatter": [
          "formatter"
        ],
        "lara-asp-graphql": [
          "graphql"
        ],
        "lara-asp-graphql-testing": [
          "graphql-testing"
        ],
        "lara-asp-migrator": [
          "migrator"
        ],
        "lara-asp-serializer": [
          "serializer"
        ],
        "lara-asp-spa": [
          "spa"
        ],
        "lara-asp-testing": [
          "testing"
        ],
        "phpunit-graphql": [
          "graphql-printer-testing"
        ],
        "text-parser": [
          "diy-parser"
        ]
      }
    },
    "laravel": {
      "providers": [
        "LastDragon_ru\\LaraASP\\Core\\PackageProvider",
        "LastDragon_ru\\LaraASP\\Dev\\PackageProvider",
        "LastDragon_ru\\LaraASP\\Documentator\\PackageProvider",
        "LastDragon_ru\\LaraASP\\Eloquent\\PackageProvider",
        "LastDragon_ru\\LaraASP\\Formatter\\PackageProvider",
        "LastDragon_ru\\LaraASP\\GraphQL\\PackageProvider",
        "LastDragon_ru\\LaraASP\\Migrator\\PackageProvider",
        "LastDragon_ru\\LaraASP\\Serializer\\PackageProvider",
        "LastDragon_ru\\LaraASP\\Spa\\PackageProvider"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer run-script dump:larastan"
    ],
    "post-update-cmd": [
      "composer run-script dump:larastan"
    ],
    "dump:larastan": [
      "LastDragon_ru\\LaraASP\\Dev\\PhpStan\\Larastan\\Extension::dump"
    ],
    "validate:commit": [
      "@phpstan",
      "@phpunit",
      "@phpcs",
      "@markdownlint"
    ],
    "validate:dependencies": [
      "@monorepo-builder",
      "@composer-dependency-analyser"
    ],
    "merge:dependencies": [
      "./vendor-bin/monorepo-builder/vendor/bin/monorepo-builder merge",
      "jq --indent 2 '.' composer.json > composer.tmp && mv composer.tmp composer.json"
    ],
    "phpstan": [
      "./vendor/bin/phpstan analyse"
    ],
    "phpunit": [
      "./vendor/bin/phpunit"
    ],
    "phpcs": [
      "./vendor-bin/phpcs/vendor/bin/phpcs"
    ],
    "markdownlint": [
      "npm run markdownlint"
    ],
    "composer-dependency-analyser": [
      "vendor-bin/composer-dependency-analyser/vendor/bin/composer-dependency-analyser --verbose --show-all-usages"
    ],
    "monorepo-builder": [
      "./vendor-bin/monorepo-builder/vendor/bin/monorepo-builder validate"
    ],
    "coverage": [
      "@php -dxdebug.mode=coverage ./vendor/bin/phpunit --coverage-html=.phpunit/coverage.html"
    ],
    "rebuild:docs": [
      "@php dev/artisan lara-asp-documentator:commands lara-asp-documentator packages/lara-asp-documentator/docs/Commands",
      "@php dev/artisan lara-asp-documentator:commands lara-asp-migrator packages/lara-asp-migrator/docs/Commands",
      "@php dev/artisan lara-asp-documentator:preprocess --exclude='**/**/*Test/**' --exclude='**/**/*Test.md' --exclude='**/**/*Test~*.md' --exclude='node_modules/' --exclude='vendor-bin/' --exclude='vendor/' --exclude='dev/' --exclude='tmp/'",
      "npm run markdownlint -- --fix || npm run markdownlint -- --fix"
    ]
  },
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "bamarni/composer-bin-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": false
    }
  },
  "replace": {
    "lastdragon-ru/glob-matcher": "self.version",
    "lastdragon-ru/graphql-printer": "self.version",
    "lastdragon-ru/lara-asp-core": "self.version",
    "lastdragon-ru/lara-asp-dev": "self.version",
    "lastdragon-ru/lara-asp-documentator": "self.version",
    "lastdragon-ru/lara-asp-eloquent": "self.version",
    "lastdragon-ru/lara-asp-formatter": "self.version",
    "lastdragon-ru/lara-asp-graphql": "self.version",
    "lastdragon-ru/lara-asp-migrator": "self.version",
    "lastdragon-ru/lara-asp-serializer": "self.version",
    "lastdragon-ru/lara-asp-spa": "self.version",
    "lastdragon-ru/lara-asp-testing": "self.version",
    "lastdragon-ru/path": "self.version",
    "lastdragon-ru/phpunit-extensions": "self.version",
    "lastdragon-ru/phpunit-graphql": "self.version",
    "lastdragon-ru/text-parser": "self.version"
  }
}
